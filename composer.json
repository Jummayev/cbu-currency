{
    "name": "cbu/currency",
    "description": "Central Bank of Uzbekistan currency rates package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jummayev Nurbek",
            "email": "jummayevnurbek279@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cbu\\Currency\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cbu\\Currency\\CbuCurrencyServiceProvider"
            ],
            "aliases": {
                "CbuCurrency": "Cbu\\Currency\\Facades\\CbuCurrency"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
